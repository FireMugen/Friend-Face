<h1>All Users</h1>
<ul class="user-accounts">
	<% @users.each do |user| %>
			<li class="home-posts">
				<span><%= cl_image_tag(user.image, :class => 'home-thumb', :default_image => "Reynholmlogo_rc8igd.png") %></span>
				<span class="home-user"><%= link_to user.name, user_path(user.id) %></span>
			</li>
	<% end %>
</ul>
